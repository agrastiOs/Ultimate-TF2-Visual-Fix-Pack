"VertexlitGeneric"
{
	"$baseTexture" "models/workshop/weapons/c_models/c_medigun_defense/c_medigun_defense"

	"$detail" "effects/tiledfire/fireLayeredSlowTiled512.vtf"	
	"$detailscale" "5"
	"$detailblendfactor" .00
	"$detailblendmode" 6
	"$yellow" "0"
	"$additive" "1"

	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"	

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "10"	
	"$lightwarptexture" "models/lightwarps/jarate_lightwarp"
	"$phongfresnelranges"	"[.3 1 8]"
	


 	"$blendtintbybasealpha" "1" 
 	//"$blendtintcoloroverbase" "1"  
	"$colortint_base" "{166 134 103}" 
	"$color2" "{69 53 48}" 
	//"$colortint_tmp" "[0 0 0]" 

	"$rimlight" "1"												// To enable rim lighting (requires phong)
	"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	"$rimlightboost" "2"		"$cloakPassEnabled" "1"

	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}

		"AnimatedTexture"
		{
			"animatedtexturevar" "$detail"
			"animatedtextureframenumvar" "$detailframe"
			"animatedtextureframerate" 30
		}

		"BurnLevel"
		{
			"resultVar" "$detailblendfactor"
		}

		"YellowLevel"
		{
			"resultVar" "$yellow"
		}

		"Equals"
		{
			"srcVar1" "$yellow"
			"resultVar" "$color2"
		}
	}
}
